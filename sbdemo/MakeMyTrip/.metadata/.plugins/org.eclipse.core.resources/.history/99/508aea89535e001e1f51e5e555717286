package com.flights.model;

import java.util.List;

public class TravelClass {
	private int travelClassId;
    private String name;
    private String description;
    
    private List<AircraftSeat> aircraftSeats;
    
	public List<AircraftSeat> getAircraftSeats() {
		return aircraftSeats;
	}
	public void setAircraftSeats(List<AircraftSeat> aircraftSeats) {
		this.aircraftSeats = aircraftSeats;
	}
	public int getTravelClassId() {
		return travelClassId;
	}
	public void setTravelClassId(int travelClassId) {
		this.travelClassId = travelClassId;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
    

}
